generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Product {
  id        Int      @id @default(autoincrement())
  title     String
  image     String
  price     Float
  favorite  Boolean  @default(false)
  createdAt DateTime @default(now())
}
